{
    "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js",
            "built/**/*.js",
            "*.js"
        ]
    },
    "rules": {
        "no-console": false,
        "no-debugger": false,
        "prefer-for-of": false
    }
}
